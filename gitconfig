[user]
	email = hitode909@gmail.com
	name = hitode909
[core]
	excludesfile = ~/.gitignore
	pager = lv -c
[github]
	user = hitode909
[alias]
        has-merged   = "!sh -c 'git rev-list HEAD | grep `git rev-parse $0`'"
        clone-branch = "!sh -c 'git checkout -t origin/$0'"
        current-branch = "!sh -c 'git name-rev --name-only HEAD'"
        cancel = !git commit -a -m 'Temporary commit for cancel' && git reset --hard HEAD~
[push]
	default = upstream
[branch]
	autosetuprebase = always
[color]
	ui = auto
[help]
	autocorrect = 0
